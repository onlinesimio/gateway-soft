{
  "name": "gateway-soft",
  "version": "1.0.0",
  "description": "modem gateway soft",
  "main": "index.js",
  "scripts": {
    "dev": "node_modules/.bin/electron-webpack dev",
    "compile": "cross-env NODE_ENV=production node_modules/.bin/electron-webpack --mode production",
    "compile:dev": "cross-env NODE_DEBUG=true node_modules/.bin/electron-webpack",
    "dist": "npm run compile && node_modules/.bin/electron-builder",
    "dist:dir": "npm run dist --dir -c.compression=store -c.mac.identity=null",
    "dist:win": "npm run compile && node_modules/.bin/electron-builder -w",
    "dist:mac": "npm run compile && node_modules/.bin/electron-builder -m",
    "dist:dev": "cross-env NODE_ENV=development npm run dist",
    "postinstall_": "node_modules/.bin/electron-builder install-app-deps",
    "postinstall": "node_modules/.bin/electron-webpack install-app-deps",
    "rebuild-all": "node_modules/.bin/electron-rebuild -f",
    "install-deps": "npm install --add-python-to-path --global --production windows-build-tools",
    "build-sqlite3-win": "export PATH=/usr/bin:$PATH && cd node_modules/sqlite3 && npm install nan && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/electron-v2.0-win32-x64 && node-gyp rebuild --target=2.0.7 --arch=x64 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v2.0-win32-x64",
    "build-sqlite3-mac": "export PATH=/usr/bin:$PATH && cd node_modules/sqlite3 && npm install nan && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/electron-v2.0-darwin-x64 && node-gyp rebuild --target=2.0.7 --arch=x64 --target_platform=darwin --dist-url=https://atom.io/download/electron --module_name=node_sqlite3 --module_path=../lib/binding/electron-v2.0-darwin-x64"
  },
  "author": "OnlineSIM",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^6.0.0",
    "electron-log": "^2.2.16",
    "electron-store": "^2.0.0",
    "knex": "^0.19.5",
    "pdu.ts": "^1.1.4",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-vue": "^0.3.3",
    "serialport": "^6.2.2",
    "source-map-support": "^0.5.8",
    "sqlite3": "^4.0.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "electron-devtools-installer": "^2.2.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/knex": "^0.14.20",
    "@types/node": "^10.7.0",
    "@types/serialport": "^6.0.4",
    "electron": "^2.0.7",
    "electron-builder": "^20.27.1",
    "electron-rebuild": "^1.8.2",
    "electron-webpack": "^2.1.2",
    "electron-webpack-eslint": "^2.1.1",
    "electron-webpack-ts": "^2.0.2",
    "electron-webpack-vue": "^2.1.3",
    "eslint-plugin-html": "^4.0.5",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "build": {
    "appId": "modem-soft",
    "productName": "modem-soft",
    "copyright": "Copyright Â© 2018 App Inc.",
    "nsis": {},
    "win": {
      "target": "nsis",
      "icon": "static/icon.ico"
    },
    "mac": {
      "target": "dmg"
    },
    "electronDownload": {
      "cache": "~/.cache/electron/"
    },
    "compression": "store"
  },
  "electronWebpack": {
    "main": {
      "extraEntries": [
        "@/../../lib/modemConnectionProcess.ts"
      ]
    }
  }
}
